{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"codegen": {
			"dependsOn": ["^codegen"],
			"outputs": ["./src/gen/**"]
		},
		"clean": {
			"inputs": ["./dist/**"],
			"outputs": []
		},
		"build": {
			"dependsOn": ["^build", "clean", "codegen"],
			"inputs": ["./src/**"],
			"outputs": ["./dist/**"]
		},
		"attw": {
			"dependsOn": ["^attw", "build"],
			"inputs": ["./dist/**", "package.json"]
		}
	}
}
