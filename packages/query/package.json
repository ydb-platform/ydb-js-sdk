{
	"name": "@ydbjs/query",
	"version": "6.0.0-alpha.13",
	"description": "High-level, type-safe YQL query and transaction client for YDB. Supports tagged template syntax, parameter binding, transactions, and statistics.",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"scripts": {
		"clean": "rm -rf dist",
		"build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
		"build:esm": "tsc --project tsconfig.json --outDir ./dist/esm",
		"test": "node --import tsx --test ./tests/**.test.ts",
		"test:watch": "node --import tsx --test --watch ./tests/**.test.ts",
		"attw": "attw --pack"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		},
		"./yql": {
			"require": "./dist/cjs/yql.js",
			"import": "./dist/esm/yql.js"
		}
	},
	"types": "dist/esm/index.d.ts",
	"typesVersions": {
		"*": {
			".": [
				"./dist/cjs/index.d.ts"
			],
			"yql": [
				"./dist/cjs/yql.d.ts"
			]
		}
	},
	"dependencies": {
		"@bufbuild/protobuf": "^2.2.5",
		"@ydbjs/api": "6.0.0-alpha.7",
		"@ydbjs/core": "6.0.0-alpha.10",
		"@ydbjs/error": "6.0.0-alpha.7",
		"@ydbjs/retry": "6.0.0-alpha.9",
		"@ydbjs/value": "6.0.0-alpha.8",
		"debug": "^4.4.0",
		"nice-grpc": "^2.1.12"
	},
	"peerDependencies": {
		"@ydbjs/api": "6.0.0-alpha.7",
		"@ydbjs/core": "6.0.0-alpha.10"
	},
	"devDependencies": {
		"@types/debug": "^4.1.12"
	},
	"publishConfig": {
		"tag": "alpha",
		"access": "public"
	}
}
