{
	"name": "@ydbjs/query",
	"version": "6.0.0-alpha.1",
	"description": "YDB Query Client",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"scripts": {
		"prebuild": "rm -rf ./dist/*",
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
		"build:esm": "tsc --project tsconfig.json --outDir ./dist/esm",
		"test": "node --experimental-strip-types --test ./tests/**.test.ts",
		"test:watch": "node --experimental-strip-types --test --watch ./tests/**.test.ts",
		"attw": "attw --pack"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"types": "dist/esm/index.d.ts",
	"dependencies": {
		"@bufbuild/protobuf": "^2.2.3",
		"@ydbjs/api": "6.0.0-alpha.1",
		"@ydbjs/core": "6.0.0-alpha.1",
		"@ydbjs/error": "6.0.0-alpha.1",
		"@ydbjs/retry": "6.0.0-alpha.1",
		"@ydbjs/value": "6.0.0-alpha.1",
		"debug": "^4.4.0",
		"nice-grpc": "^2.1.12"
	},
	"peerDependencies": {
		"@ydbjs/api": "6.0.0-alpha.1",
		"@ydbjs/core": "6.0.0-alpha.1"
	},
	"devDependencies": {
		"@types/debug": "^4.1.12"
	}
}
