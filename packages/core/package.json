{
	"name": "@ydbjs/core",
	"version": "6.0.0-alpha.4",
	"description": "YDB Core",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"scripts": {
		"build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
		"build:esm": "tsc --project tsconfig.json --outDir ./dist/esm",
		"test": "node --import tsx --test ./tests/**.test.ts",
		"test:watch": "node --import tsx --test --watch",
		"attw": "attw --pack"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"types": "dist/esm/index.d.ts",
	"dependencies": {
		"@bufbuild/protobuf": "^2.2.3",
		"@ydbjs/api": "6.0.0-alpha.4",
		"@ydbjs/auth": "^6.0.0-alpha.4",
		"@ydbjs/error": "6.0.0-alpha.4",
		"@ydbjs/retry": "6.0.0-alpha.4",
		"debug": "^4.4.0",
		"nice-grpc": "^2.1.12"
	},
	"devDependencies": {
		"@types/debug": "^4.1.12"
	},
	"publishConfig": {
		"tag": "alpha",
		"access": "public"
	}
}
